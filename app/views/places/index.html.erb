<% title "Places" %>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Address</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Altitude</th>
    <th>Venue Type</th>
  </tr>
  <% for place in @places %>
    <tr>
      <td><%=h place.name %></td>
      <td><%=h place.description %></td>
      <td><%=h place.address %></td>
      <td><%=h place.latitude %></td>
      <td><%=h place.longitude %></td>
      <td><%=h place.altitude %></td>
      <td><%=h place.venue_type_id %></td>
      <td><%= link_to "Show", place %></td>
      <td><%= link_to "Edit", edit_place_path(place) %></td>
      <td><%= link_to "Destroy", place, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Place", new_place_path %></p>
